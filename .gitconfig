[branch "master"]
  remote = origin
  merge = refs/heads/master
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = green bold
  changed = yellow bold
  untracked = white bold
[user]
  email = jamison@lahman.dev
  name = Jamison Lahman
[push]
  default = current
[pull]
  rebase = true
[rebase]
  autostash = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[init]
	defaultBranch = master
[core]
	hooksPath = /home/jamison/.githooks
[url "ssh://git@github.com/"]
  insteadOf = https://github.com/
